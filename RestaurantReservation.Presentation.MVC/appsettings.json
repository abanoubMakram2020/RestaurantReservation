{
  "DatabaseConfiguration": {
    "ResturantReservationDBConnection": "Server=.;Database=ResturantReservationDB;Trusted_Connection=True;MultipleActiveResultSets=true",
    "ResturantReservationIdentityDBConnection": "Server=.;Database=ResturantReservationIdentityDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "MinimumLevel": [ "Debug", "Error", "Warning" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logger//log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
